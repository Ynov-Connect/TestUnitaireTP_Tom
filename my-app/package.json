{
  "name": "ynov-inscription-form",
  "version": "2.0.0",
  "main": "dist/index.js",
  "homepage": "https://ynov-connect.github.io/TestUnitaireTP_Tom",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "axios": "^1.13.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^6.30.3",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --setupFiles=./.jest/setEnvVars.js",
    "test:e2e": "start-server-and-test start http://localhost:3000 cy:run",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "jsdoc": "jsdoc -R ./README.md -c ./jsdoc.config.json -r -d ./public/docs",
    "build-npm": "cross-env NODE_ENV=production rimraf dist && babel src --out-dir dist --copy-files --ignore \"**/*.test.js,**/*.spec.js,**/*.test.jsx,**/*.spec.jsx\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-react-jsx": "^7.28.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "cross-env": "^10.1.0",
    "cypress": "^15.10.0",
    "jsdoc": "^4.0.5",
    "react-test-renderer": "^19.2.4",
    "rimraf": "^6.1.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/index.js",
      "src/reportWebVitals.js",
      "src/setupTests.js"
    ]
  }
}
