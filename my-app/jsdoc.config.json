{
  "source": {
    "include": ["src/"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|setupTests|reportWebVitals|index\\.js|App\\.js|\\.test\\.js)"
  },
  "opts": {
    "destination": "./public/docs",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
